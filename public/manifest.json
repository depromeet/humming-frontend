{
  "name": "Humming",
  "version": "1.0",
  "description": "Recommended song according to weather.",
  "permissions": ["declarativeContent", "storage", "geolocation"],
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "play.png",
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-sPGY2rm4DpViU6dGhPvD2571GGjx05TVE/cSbPyY0L8='; object-src 'self'"
}
